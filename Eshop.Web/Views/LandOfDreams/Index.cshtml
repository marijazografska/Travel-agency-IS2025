@model List<Dictionary<string, object>>

@{
    ViewData["Title"] = "Hotel Listings";
}

<h2>@ViewData["Title"]</h2>

<table class="table">
    <thead>
    <tr>
        <th>Name</th>
        <th>Price</th>
        <th>Location</th>
        <th>Map</th>
        <th>Description</th>
        <th>Image</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var hotel in Model)
    {
        <tr>
            <td>@hotel["Name"]</td>
            <td>@hotel["Price"]</td>
            <td>@hotel["Location"]</td>
            <td>@Html.Raw(hotel["Map"])</td> <!-- Embed the map iframe -->
            <td>@hotel["Description"]</td>
            <td><img src="@hotel["ImageUrl"]" alt="@hotel["Name"]" style="width: 100px; height: auto;" /></td>
        </tr>
    }
    </tbody>
</table>